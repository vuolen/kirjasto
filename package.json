{
  "name": "kirjasto",
  "version": "1.0.0",
  "description": "Kirjasto is a library management system.",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "start-dev": "./dev-env.sh up -d frontend backend db && wait-on https://kirjasto:8001",
    "stop-dev": "./dev-env.sh down",
    "reset-db": "echo 'TRUNCATE book,author CASCADE' | ./dev-env.sh exec -T db psql -U postgres",
    "e2e": "cypress run",
    "e2e-gui": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuolen/kirjasto.git"
  },
  "author": "Lennu Vuolanne",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/vuolen/kirjasto/issues"
  },
  "homepage": "https://github.com/vuolen/kirjasto#readme",
  "dependencies": {
    "@cucumber/cucumber": "^7.0.0",
    "@types/cucumber": "^7.0.0",
    "@types/selenium-webdriver": "^4.0.10",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "typescript": "^4.1.3",
    "cypress": "^6.4.0",
    "wait-on": "^5.2.1"
  }
}
